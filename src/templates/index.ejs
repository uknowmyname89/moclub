<body>

  <script src="https://api-maps.yandex.ru/2.1/?lang=ru_RU"></script>

  <div class="bg-blue pt-5">
    <div class="container">
      <%- include modules/header.ejs %>
      <%- include modules/menu.ejs %>
      <div class="position-relative">
        <a href="#" class="pic-slider-prev slick-arrow slick-prev"></a>
        <a href="#" class="pic-slider-next slick-arrow slick-next"></a>
        <div class="pic-slider b-radius">
          <div class="pic-block">
            <p class="font-weight-bold fz-txt_52px text-white">
              ВСЕГО В 12 КМ <br>
